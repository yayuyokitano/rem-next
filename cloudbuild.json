{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/git",
      "entrypoint": "/bin/bash",
      "args": ["-c", "git diff --name-only $COMMIT_SHA~1 > /workspace/git-diff.txt"]
    },
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "args": [
        "bash", "deploy.sh"
      ]
    }
  ],
  "timeout": "60000s"
}